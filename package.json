{
  "name": "create-react-tsx-fc",
  "displayName": "create-react-tsx-fc",
  "description": "Create .tsx for new <MyComponent/>",
  "publisher": "mvkdev",
  "repository": "https://github.com/mvklingeren/create-fc-from-tag",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.fcFromTag"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.fcFromTag",
        "title": "React FC from <Tag/>"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
